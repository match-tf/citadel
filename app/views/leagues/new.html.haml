- content_for(:title) { 'Create Tournament' }

.panel.panel-container
  .row
    = bootstrap_form_for @league, url: leagues_path(@league) do |f|
      = render 'league_form', f: f, weekly_scheduler: @weekly_scheduler
    
      = f.submit 'Create'
      = link_to 'Cancel', admin_path, class: 'btn btn-default'
